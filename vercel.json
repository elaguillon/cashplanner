{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api"
    }
  ],
  "public": true,
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ]
}
